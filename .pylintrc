[MASTER]
init-hook='import sys; sys.path.append("/.pylintrc")'

[FORMAT]
max-line-length=120
disable=missing-module-docstring,
        missing-function-docstring,
        missing-class-docstring,
        too-many-arguments,
        too-few-public-methods,
        chained-comparison,
        no-else-break,
        no-else-return,
        import-error,
        unused-variable,
        too-many-public-methods,
        too-many-statements,
        too-many-locals
